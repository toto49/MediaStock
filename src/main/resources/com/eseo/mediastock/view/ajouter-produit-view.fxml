<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="com.eseo.mediastock.controller.AjouterProduitController"
            prefHeight="400.0" prefWidth="600.0">


    <ComboBox fx:id="ChoiceAddProduit" layoutX="15.0" layoutY="33.0" prefWidth="90.0" promptText="Produit">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="LIVRES"/>
                <String fx:value="DVD"/>
                <String fx:value="JEUX"/>
            </FXCollections>
        </items>
    </ComboBox>
    <VBox fx:id="containerFormulaire" layoutX="15.0" layoutY="80.0" spacing="10.0" prefWidth="300.0">
    </VBox>
</AnchorPane>
